{
  "version": 2,
  "name": "monorepo",
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "built-storybooks" }
    }
  ],
  "routes": [
    { "src": "/docs", "dest": "https://storybook.js.org" },
    { "src": "/(.*)", "dest": "/built-storybooks/$1" }
  ]
}
