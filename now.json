{
  "version": 2,
  "name": "monorepo",
  "public": true,
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { 
        "distDir": "built-storybooks",
        "trailingSlash": true,
        "renderSingle": true
       }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/built-storybooks/$1" }
  ]
}
