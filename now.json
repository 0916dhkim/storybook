{
  "version": 2,
  "name": "monorepo",
  "public": true,
  "static": {
    "cleanUrls": false,
    "trailingSlash": true
  },
  "github": {
    "silent": true
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { 
        "distDir": "built-storybooks"
       }
    }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/built-storybooks/$1" }
  ]
}
