{
  "name": "@storybook/addon-onboarding",
  "version": "1.0.11",
  "description": "Storybook Addon Onboarding - Introduces a new onboarding experience",
  "keywords": [
    "storybook-addons",
    "addon-onboarding"
  ],
  "homepage": "https://github.com/storybookjs/addon-onboarding#readme",
  "bugs": {
    "url": "https://github.com/storybookjs/addon-onboarding/issues"
  },
  "repository": "git+https://github.com/storybookjs/addon-onboarding.git",
  "license": "MIT",
  "author": "@storybookjs/core",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./package.json": "./package.json",
    "./manager": "./dist/manager.mjs",
    "./preset": "./dist/preset.js"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*",
    "README.md",
    "*.js",
    "*.d.ts"
  ],
  "scripts": {
    "prebuild": "rimraf ./dist",
    "build": "tsup",
    "build-storybook": "storybook build",
    "build:watch": "pnpm run build --watch",
    "chromatic": "npx chromatic",
    "release": "pnpm run build && auto shipit",
    "start": "run-p build:watch 'storybook --quiet'",
    "storybook": "storybook dev -p 6006",
    "storybook:watch": "nodemon"
  },
  "nodemonConfig": {
    "exec": "pnpm run storybook --ci",
    "ext": "js,jsx,ts,tsx",
    "ignore": [
      "src/stories",
      "src/**/*.stories.*"
    ],
    "watch": [
      "src",
      ".storybook/main.ts",
      "vite.config.ts"
    ]
  },
  "dependencies": {
    "@storybook/telemetry": "next",
    "react-confetti": "^6.1.0"
  },
  "devDependencies": {
    "@radix-ui/react-dialog": "^1.0.5",
    "@storybook/addon-essentials": "next",
    "@storybook/addon-interactions": "next",
    "@storybook/addon-links": "next",
    "@storybook/blocks": "next",
    "@storybook/channels": "next",
    "@storybook/components": "next",
    "@storybook/core-events": "next",
    "@storybook/jest": "^0.2.3",
    "@storybook/manager": "next",
    "@storybook/manager-api": "next",
    "@storybook/preview": "next",
    "@storybook/react": "next",
    "@storybook/react-vite": "next",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/theming": "next",
    "@storybook/types": "next",
    "@types/node": "^18",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "auto": "^11.0.4",
    "chromatic": "^10.5.0",
    "dedent": "^1.5.1",
    "framer-motion": "^11.0.3",
    "nodemon": "^3.0.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-joyride": "^2.7.2",
    "react-use-measure": "^2.1.1",
    "rimraf": "^5.0.5",
    "storybook": "next",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.12"
  },
  "bundler": {
    "exportEntries": [
      "src/index.ts"
    ],
    "managerEntries": [
      "src/manager.tsx"
    ],
    "nodeEntries": [
      "./src/preset.ts"
    ]
  },
  "publishConfig": {
    "access": "public"
  }
}
